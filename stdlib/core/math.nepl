//:| #entry main
//:| #target wasi
//:| #import "std/test" as *
//:| #import "core/math" as *
//: fn main <()*>()> ():
//:     assert_eq_i32 3 u32_add 1 2;
//:     assert u64_lt i64_extend_i32_u 9 i64_extend_i32_u 10;
//: ```

//: u32_add: u32 の加算（mod 2^32）
