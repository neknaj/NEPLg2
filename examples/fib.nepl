#entry main
#indent 4

#import "std/math"
#use std::math::*
#if[target=wasi]
#import "std/stdio"
#use std::stdio::*

fn main <()*> ()> ():
    let mut a <i32> 0;
    let mut b <i32> 1;
    let mut i <i32> 0;
    while lt i 10:
        print_str "fib = ";
        print_i32 a;
        let mut next add a b;
        set a b;
        set b next;
        set i add i 1;
        print_str "\n";
